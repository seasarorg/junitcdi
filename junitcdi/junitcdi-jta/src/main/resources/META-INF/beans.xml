<beans xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/beans_1_0.xsd">
	<policies>
		<class>org.seasar.junitcdi.jta.internal.DataSourceValidator</class>
		<class>org.seasar.junitcdi.jta.internal.TransactionCommittingInterceptor</class>
		<class>org.seasar.junitcdi.jta.internal.TransactionManagerProducer</class>
		<class>org.seasar.junitcdi.jta.internal.TransactionRollingbackInterceptor</class>
		<annotation>org.seasar.junitcdi.jta.Transactional</annotation>
	</policies>
	<interceptors>
		<class>org.seasar.junitcdi.jta.internal.TransactionCommittingInterceptor</class>
		<class>org.seasar.junitcdi.jta.internal.TransactionRollingbackInterceptor</class>
	</interceptors>
</beans>
